.contains {
  width: 100%;
  background-color: #f5f5f5;
}
.header {
  width: 100%;
  height: 300rpx;
  background: url("https://static.shengri.cn/uploads/xydd/web/xydd_heart.png") no-repeat;
  background-size: cover;
  padding-top: 1rpx;
}
.header_cover {
  display: block;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1px solid #eee;
  margin: 60rpx auto 0;
}
.header_title {
  font-family: PingFangSC-Regular;
  color: #fff;
  font-size: 36rpx;
  padding-top: 15rpx;
  width: 100%;
  text-align: center;
}
.header_info {
  font-family: PingFangSC-Light;
  color: #fff;
  font-size: 28rpx;
  padding-top: 5rpx;
  width: 100%;
  text-align: center;
}
.zhubo_info {
  width: 100%;
  background-color: #fff;
  overflow: hidden;
  padding: 30rpx 30rpx 60rpx;
}
.left_img {
  width: 30rpx;
  height: 30rpx;
  float: left;
}
.right_title {
  font-family: PingFangSC-Light;
  color: #666;
  font-size: 28rpx;
  line-height: 40rpx;
  margin-left: 60rpx;
  width: 620rpx;
}

.diantai_list {
  margin-top: 20rpx;
  padding-bottom: 40rpx;
  width: 100%;
  background-color: #fff;
}
.title {
	width: 100%;
	height: 70rpx;
}
.title_left {
	float: left;
	background-color: #f0626e;
	width: 10rpx;
	height: 70rpx;
}
.title_name {
	font-size: 30rpx;
	line-height: 70rpx;
	color: #555;
	padding-left: 30rpx;
	font-weight: bolder;
}
.list_detail {
  margin: 0rpx 30rpx;
  border-bottom: 1px #e9e9e9 solid;
  height: 130rpx;
}
.zhubo_img {
  width: 90rpx;
  height: 90rpx;
  float: left;
  padding-top: 20rpx;
}
.zhubo_title {
  float: left;
  padding-top: 20rpx;
  padding-left: 40rpx;
  color: #333;
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  line-height: 40rpx;
  width: 560rpx;
  height: 40rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.zhubo_viewNum {
  float: left;
  padding-top: 10rpx;
  padding-left: 40rpx;
  color: #999;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  line-height: 40rpx;
  width: 560rpx;
}







